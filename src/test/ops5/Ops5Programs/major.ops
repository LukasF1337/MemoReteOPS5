; MAJORS.OPS(literalize student        name        id        maj-code)(literalize majors        major-code        major-name)(startup        (make student ^name Laura ^id 2233 ^maj-code 10)        (make student ^name Laura ^id 1234 ^maj-code 11)        (make student ^name John ^id 4321 ^maj-code 11)        (make student ^name Heather ^id 3214 ^maj-code 10)        (make student ^name Becca ^id 9876 ^maj-code 10)        (make student ^name Kathy ^id 6543 ^maj-code 11)        (make majors ^major-code 10 ^major-name biology)        (make majors ^major-code 11 ^major-name chemistry))(p select-major        (student ^name <name> ^maj-code <mc> ^id <id>)        (majors ^major-code <mc> ^major-name <major>)      - (student ^maj-code < <mc>)-->        (write <name> (tabto 20) <major> (tabto 30) <id> (crlf))        (remove 1))