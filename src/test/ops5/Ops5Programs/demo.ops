; DEMO.OPS(literalize city        name        state)(literalize person        name        city        state)        (literalize list-time)(startup ;startup ist ein VAX-OPS5 Befehl        (make person ^name irv ^city ipswich ^state MA)        (make person ^name john ^city hendersonville ^state TN)        (make person ^name brook ^city |los angeles| ^state CA)        (make person ^name debbie ^city ipswich ^state MA)        (make person ^name janet ^city beverly ^state MA)        (make person ^name jonathon ^city hamilton ^state MA)        (make person ^name karen ^city Ipswich ^state MA))(p add-a-city-to-wm        {<person>(person ^city <theCity> ^state <theState>)}      - (city ^name <theCity>)-->        (make city ^name <theCity> ^state <theState>)        (remove  <person>))(p irrelevant-person-info        {<person>(person ^city <theCity>)}        (city ^name <theCity>)-->        (remove <person>))(p shift-context      - (person)        (city)-->        (make list-time))(p list        (list-time)        {<city> (city ^name <theCity> ^state <theState>)}-->        (remove <city>)        (write <theCity> |, | <theState> (crlf)))(p list-finished        (list-time)        -(city)-->        (halt))